{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["google", "plugin:jsdoc/recommended", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "jsdoc"],
  "settings": {
    "jsdoc/ignore-internal": 1
  },
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true, "printWidth": 200 }],
    "quotes": ["error", "single"],
    "max-len": ["error", { "code": 200 }],
    "valid-jsdoc": 0,
    "require-jsdoc": 0
  }
}
